{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "error",
        "no-unused-expressions":"error",
        "prefer-const":"error"
     
      // Add your custom rules here
    },
    "globals": {
        "process":"readonly"

    }
  }
  